@using ASFY_Proyecto.Models;
@{
    ViewBag.Title = "Programas";
}

<br>
<br>

@foreach (Programas programas in ViewBag.Programas)
{

    <div class="container">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Numero de Programa
                </tr>
            </thead>
            <tbody>
                <tr>
                    <th scope="row">1</th>
                    <td>@programas.Nombre</td>
                </tr>
                <tr>
                    <th scope="row">2</th>
                    <td>@programas.Descripcion</td>
                </tr>
            </tbody>
        </table>

    </div>
}



<br>
<br>
