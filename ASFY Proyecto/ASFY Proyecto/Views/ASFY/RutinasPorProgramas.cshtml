@using ASFY_Proyecto.Models;
@{
    ViewBag.Title = "RutinasPorProgramas";
}


<br>
<br>

<div class="container">
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Nombre</th>
                <th scope="col">Repeticiones</th>

                <th scope="col">

                </th>
            </tr>
        </thead>

        @foreach (Rutinas rutinas in ViewBag.RutinasMostrarProgramas)
        {

    <thead class="thead-light">
        @foreach (Programas programas in ViewBag.Programas)
        {

            <tr>
                <td>@rutinas.Nombre</td>
                <td>15 minutos </td>

                <td>@Html.ActionLink("Video", "LinkVideo", "ASFY", new { codigoProgramas = programas.codigo }, null) </td>


            </tr>
        }
        </thead>
            }
        
    </table>

</div>
<br>
